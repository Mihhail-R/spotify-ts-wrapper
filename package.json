{
	"name": "spotify-api-wrapper",
	"version": "1.0.0",
	"description": "Spotify api wrapper written in typescript",
	"main": "src/index.ts",
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"fix": "prettier-eslint --write \"src/**/*.ts\"",
		"lint": "prettier-eslint --list-different \"src/**/*.ts\"",
		"test": "jest",
		"prepare": "husky install",
		"upgrade-packages": "npx npm-check-updates -u && npm i",
		"build": "tsc"
	},
	"author": "Mihhail-R",
	"license": "MIT",
	"homepage": "",
	"devDependencies": {
		"@jaanjah/eslint-config": "^1.2.1",
		"@jaanjah/prettier-config": "^1.0.1",
		"@swc/cli": "^0.1.62",
		"@swc/core": "^1.3.82",
		"@swc/jest": "^0.2.29",
		"@types/jest": "^29.5.4",
		"@types/node": "^18",
		"axios": "^1.5.0",
		"dotenv": "^16.3.1",
		"husky": "^8.0.3",
		"jest": "^29.6.4",
		"prettier": "^2.8.3",
		"prettier-eslint-cli": "^7.1.0",
		"pretty-quick": "^3.1.3",
		"typescript": "^5.2.2"
	},
	"lint-staged": {
		"src/**/*.ts": [
			"npm run lint"
		]
	}
}

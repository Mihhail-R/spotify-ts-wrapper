{
	"testEnvironment": "node",
	"transform": {
		"^.+\\.(t|j)sx?$": "@swc/jest"
	},
	"collectCoverage": true,
	"coverageReporters": ["json", "lcov", "text", "clover"],
	"coverageDirectory": "coverage",
	"coverageThreshold": {
		"global": {
			"branches": 80,
			"functions": 80,
			"lines": 80,
			"statements": 80
		}
	},
	"coveragePathIgnorePatterns": [
		"/node_modules/", "/test/", "/coverage/", "/lib/"
	]
}
